{
    "name": "Dart",
    "id": "dart-sdk",
    "version": "0.0.2",
    "description": "Install the Dart SDK",
    "documentationURL": "https://github.com/FoodieAppServices/vsc-features/blob/main/src/dart-sdk",
    "containerEnv": {
        "DART_HOME": "$HOME/dart-sdk",
        "DART_ROOT": "$DART_HOME",
        "PUB_CACHE": "$DART_HOME/.pub-cache",
        "PATH": "$PATH:$DART_HOME/bin:$PUB_CACHE/bin"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.dart-code"
            ]
        }
    },
    "options": {}
}